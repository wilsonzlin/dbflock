{
  "name": "dbflock",
  "version": "0.6.0",
  "description": "Migrate, change, apply, and manage PostgreSQL database schemas",
  "bin": {
    "dbflock": "dist/main.js"
  },
  "main": "dist/main.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "npm run clean && tsc && chmod +x dist/main.js",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "prettier --check 'src/**/*.ts' && npm run build",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wilsonzlin/dbflock.git"
  },
  "keywords": [
    "change",
    "database",
    "db",
    "downgrade",
    "migrate",
    "migration",
    "mysql",
    "postgres",
    "postgresql",
    "relational",
    "schema",
    "sql",
    "update",
    "upgrade"
  ],
  "author": {
    "email": "npm@wilsonl.in",
    "name": "Wilson Lin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wilsonzlin/dbflock/issues"
  },
  "homepage": "https://github.com/wilsonzlin/dbflock#readme",
  "dependencies": {
    "pg": "^8.5.1",
    "pg-native": "^3.0.0",
    "sacli": "^2.0.0"
  },
  "optionalDependencies": {
    "pg-native": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "@types/pg": "^7.14.8",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  }
}
